{
  "name": "redis-cache",
  "version": "1.0.0",
  "description": "A cache service using redis.",
  "main": "index.js",
  "scripts": {
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint": "eslint . --ext .ts",
    "dev": "concurrently \"nodemon\" \"npm:docker\"",
    "build": "tsc",
    "docker": "docker-compose -f redis-compose.yml up",
    "test": "mocha --exit -r ts-node/register 'src/tests/**/*.ts'",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add .",
    "postversion": "git push && git push --tags",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterlogo/redis-cache.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [],
  "author": "Peter Logo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/peterlogo/redis-cache/issues"
  },
  "homepage": "https://github.com/peterlogo/redis-cache#readme",
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.27",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "chai": "^4.3.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.0.9",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "redis": "^3.0.2"
  }
}
